    "date-fns": "^3.3.1",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "sonner": "^1.4.0",
    "tailwind-merge": "^2.2.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@tszhong0411/eslint-config": "^0.0.3",
    "@tszhong0411/prettier-config": "^0.0.1",
    "@tszhong0411/tsconfig": "^0.0.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,cjs,mjs,ts,tsx}": [
